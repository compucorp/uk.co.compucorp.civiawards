<?xml version="1.0"?>
<extension key="uk.co.compucorp.civiawards" type="module">
  <file>civiawards</file>
  <name>CiviAwards</name>
  <description>Create Awards/Grants, Manage Applications and processes</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Compuco</author>
    <email>hello@compuco.io</email>
  </maintainer>
  <urls>
    <url desc="Documentation">https://github.com/compucorp/uk.co.compucorp.civiawards/blob/master/readme.md</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2025-02-07</releaseDate>
  <version>2.4.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.51</ver>
  </compatibility>
  <civix>
    <namespace>CRM/CiviAwards</namespace>
    <format>23.02.1</format>
  </civix>
  <requires>
    <ext>uk.co.compucorp.civicase</ext>
  </requires>
  <mixins>
    <mixin>ang-php@1.0.0</mixin>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
    <mixin>entity-types-php@1.0.0</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_CiviAwards_Upgrader</upgrader>
</extension>
